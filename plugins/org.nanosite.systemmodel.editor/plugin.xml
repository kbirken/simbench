<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="SystemModel.presentation.SystemModelModelWizardID"
            name="%_UI_SystemModelModelWizard_label"
            class="SystemModel.presentation.SystemModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SystemModelModelFile.gif">
         <description>%_UI_SystemModelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="SystemModel.presentation.SystemModelEditorID"
            name="%_UI_SystemModelEditor_label"
            icon="icons/full/obj16/SystemModelModelFile.gif"
            extensions="systemmodel"
            class="SystemModel.presentation.SystemModelEditor"
            contributorClass="SystemModel.presentation.SystemModelActionBarContributor">
      </editor>
   </extension>

</plugin>
